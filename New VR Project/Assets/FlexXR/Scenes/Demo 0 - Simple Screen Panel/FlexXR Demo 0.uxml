    <Style src="project://database/Assets/FlexXR/Resources/flexxr-demo-unity-root-styles-overrides.uss?fileID=7433441132597879392&amp;guid=c4c497b636654d4abdfbbd1547ca9de0&amp;type=3#flexxr-demo-unity-root-styles-overrides" />
    <ui:VisualElement style="align-items: center; justify-content: flex-start; height: 100%; width: 100%;">
        <FlexXR.Runtime.FlexXRPanel.FlexXRContainer name="FlexXRContent" class="dp04-solid" style="-unity-background-scale-mode: flex-start; width: 465px; align-items: flex-start; justify-content: flex-start; height: auto; max-height: 50%; position: absolute; top: 0; left: 0; right: 0; bottom: 0; background-color: rgba(255, 227, 171, 0.9);">
            <ui:VisualElement name="VisualElement" style="width: 465px; align-items: flex-start; justify-content: flex-start; max-height: none;">
                <ui:VisualElement name="Header" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); flex-direction: row; margin-bottom: 10px;">
                    <ui:Label tabindex="-1" text="Block AT 178: Front Pediment Tympanon (S)" display-tooltip-when-elided="true" style="-unity-font-style: bold; margin-left: 10px; color: rgb(0, 0, 0);" />
                </ui:VisualElement>
                <ui:ScrollView scroll-deceleration-rate="0" class="base-gemplay-list-with-scrollbar" style="width: 98%;">
                    <ui:Foldout text="Block Description" name="Intro" class="foldout-toggle-panel" style="font-color: rgba(0, 0, 0, 0); margin-bottom: 10px; margin-top: 0; margin-right: 5px; align-items: flex-start; flex-direction: column; align-self: flex-start; justify-content: flex-end; flex-wrap: nowrap; flex-shrink: initial;">
                        <ui:Label text="This is the central pediment tympanon block of the front façade. It bears sculptural decoration with a male bust in a wreath  in the center, held by two flying Victories. The male figure bears the cone-capped quiver and the bow, iconographic features of Apollo, a god  whom Emperor Caracalla appealed to for healing, and who appears in coinage minted during the same period. Contrary to this pediment, the back pediment was constructed by multiple blocks and was left blank." display-tooltip-when-elided="true" style="white-space: normal; margin-bottom: 0; color: rgb(0, 0, 0);" />
                    </ui:Foldout>
                    <ui:Foldout text="Block Placement" name="ThisDemo" class="foldout-toggle-panel" style="margin-bottom: 10px; margin-top: 0; margin-right: 5px; flex-direction: column; width: 448px; align-items: stretch; justify-content: flex-start; align-content: flex-start; align-self: auto; color: rgb(0, 0, 0);">
                        <ui:Label text="The pediment block was found at the southwest of the temple, and is restored at the front façade of the temple." display-tooltip-when-elided="true" style="white-space: normal; margin-bottom: 0; color: rgb(0, 0, 0);" />
                    </ui:Foldout>
                </ui:ScrollView>
            </ui:VisualElement>
